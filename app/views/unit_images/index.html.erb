<p id="notice"><%= notice %></p>

<h1>Unit Images</h1>

<table>
  <thead>
    <tr>
      <th>Unit</th>
      <th>Url</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @unit_images.each do |unit_image| %>
      <tr>
        <td><%= unit_image.unit_id %></td>
        <td><%= unit_image.url %></td>
        <td><%= unit_image.name %></td>
        <td><%= link_to 'Show', unit_image %></td>
        <td><%= link_to 'Edit', edit_unit_image_path(unit_image) %></td>
        <td><%= link_to 'Destroy', unit_image, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Unit Image', new_unit_image_path %>
