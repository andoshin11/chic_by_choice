%p#notice= notice
%h1 Unit Images
%table
  %thead
    %tr
      %th Unit
      %th Url
      %th Name
      %th{:colspan => "3"}
  %tbody
    - @unit_images.each do |unit_image|
      %tr
        %td= unit_image.unit_id
        %td= unit_image.url
        %td= unit_image.name
        %td= link_to 'Show', unit_image
        %td= link_to 'Edit', edit_unit_image_path(unit_image)
        %td= link_to 'Destroy', unit_image, method: :delete, data: { confirm: 'Are you sure?' }
%br/
= link_to 'New Unit Image', new_unit_image_path
